CREATE TABLE EMPLEADO
(
COD_EMPLEADO NUMBER(7) NOT NULL,
EMP_NOMBRE VARCHAR(50),
EMP_APELLIDO VARCHAR(50),
EMP_TELEFONO NUMBER(10),
EMP_DIRECCION VARCHAR(50),
EMP_CIUDAD VARCHAR(50),
EMP_SALARIO NUMBER(10),
EMP_CARGO VARCHAR(50),
EMP_ANTIGUEDAD NUMBER(10),
PRIMARY KEY(COD_EMPLEADO));
COMMIT;

CREATE TABLE USUARIO
(
COD_USUARIO NUMBER(7) NOT NULL,
COD_EMPLEADO NUMBER(7) NOT NULL,
USU_USUARIO VARCHAR(50),
USU_PASSWORD VARCHAR(50),
USU_TIPO_USUARIO VARCHAR(50),
PRIMARY KEY (COD_USUARIO),
FOREIGN KEY (COD_EMPLEADO) REFERENCES EMPLEADO);
COMMIT;

CREATE TABLE TEMPORADA
(
COD_TEMPORADA NUMBER(7) NOT NULL,
TMP_UNO VARCHAR(50),
TMP_UNO_FECH_INI DATE,
TMP_UNO_FECH_FIN DATE,
TMP_DOS VARCHAR(50),
TMP_DOS_FECH_INI DATE,
TMP_DOS_FECH_FIN DATE,
TMP_TRES VARCHAR(50),
TMP_TRES_FECH_INI DATE,
TMP_TRES_FECH_FIN DATE,
PRIMARY KEY(COD_TEMPORADA));
COMMIT;

CREATE TABLE FACTURA
(
COD_FACTURA NUMBER(7) NOT NULL,
FAC_FECHA DATE,
FAC_MONTO_FACTURA NUMBER(10),
FAC_DETALLE VARCHAR(150),
FAC_CANTIDAD NUMBER(10),
FAC_APELLIDO VARCHAR(50),
PRIMARY KEY (COD_FACTURA));
COMMIT;

CREATE TABLE PROVEEDORES
(
COD_PROVEEDOR NUMBER(7) NOT NULL,
PROV_NOMBRE_CONTACTO VARCHAR(50),
PROV_DIRECCION VARCHAR(80),
PROV_TELEFONO NUMBER(15),
PROV_E_MAIL VARCHAR(50),
PROV_CIUDAD VARCHAR(50),
PROV_PAIS VARCHAR(50),
PRIMARY KEY (COD_PROVEEDOR));
COMMIT;

CREATE TABLE ADQUISICION
(
COD_ADQUISICION NUMBER(7) NOT NULL,
ADQ_FECHA DATE,
ADQ_CANTIDAD NUMBER(10),
ADQ_PRECIO_UNITARIO NUMBER(10),
ADQ_PRECIO_TOTAL NUMBER(10),
PRIMARY KEY (COD_ADQUISICION));
COMMIT;

CREATE  TABLE ESTADO_VEHICULO
(
COD_ESTADO NUMBER(7) NOT NULL,
EST_NUEVO VARCHAR(10),
EST_SEMINUEVO VARCHAR(10),
EST_USADO VARCHAR(10),
PRIMARY KEY (COD_ESTADO));
COMMIT;

CREATE  TABLE CLIENTE
(
COD_CLIENTE NUMBER(7) NOT NULL,
CLI_CI VARCHAR(15),
CLI_NOMBRE VARCHAR(50),
CLI_APELLIDOS VARCHAR(50),
CLI_DIRECCION VARCHAR(80),
CLI_TELEFONO NUMBER(10),
CLI_CIUDAD VARCHAR(50),
CLI_NIT_CLIENTE NUMBER(10),
PRIMARY KEY (COD_CLIENTE));
COMMIT;

CREATE TABLE OFERTA
(
COD_OFERTA NUMBER(7) NOT NULL,
COD_CLIENTE NUMBER(7) NOT NULL,
OFT_FECHA DATE,
OFT_MONTO NUMBER(10),
PRIMARY KEY (COD_OFERTA),
FOREIGN KEY (COD_CLIENTE) REFERENCES CLIENTE);
COMMIT;

CREATE TABLE PROV_ADQ
(
COD_PROV_ADQ NUMBER(7) NOT NULL,
COD_PROVEEDOR NUMBER(7) NOT NULL,
COD_ADQUISICION NUMBER(7) NOT NULL,
PRIMARY KEY (COD_PROV_ADQ),
FOREIGN KEY (COD_PROVEEDOR) REFERENCES PROVEEDORES,
FOREIGN KEY (COD_ADQUISICION) REFERENCES ADQUISICION);
COMMIT;

CREATE TABLE FINANCIAMIENTO
(
COD_FINANCIAMIENTO NUMBER(7) NOT NULL,
FIN_NOMBRE_BANCO VARCHAR(50),
FIN_NOMBRE_GARANTE VARCHAR(50),
FIN_APELLIDO_GARANTE VARCHAR(50),
FIN_DIRECCION VARCHAR(80),
FIN_TELEFONO NUMBER(10),
FIN_MONTO_INICIAL NUMBER(10),
FIN_MONTO_CUOTA NUMBER(10),
FIN_CANTIDAD_CUOTAS NUMBER(10),
FIN_MONTO_TOTAL NUMBER(10),
FIN_FECHA_INI DATE,
FIN_FECHA_FIN DATE,
PRIMARY KEY (COD_FINANCIAMIENTO));
COMMIT;

CREATE TABLE VENTA
(
COD_VENTA NUMBER(7) NOT NULL,
COD_CLIENTE NUMBER(7) NOT NULL,
COD_EMPLEADO NUMBER(7) NOT NULL,
COD_FACTURA NUMBER(7) NOT NULL,
COD_TEMPORADAS NUMBER(7) NOT NULL,
VEN_VALOR NUMBER(10),
VEN_FECHA DATE,
VEN_CANTIDAD NUMBER(10),
VEN_MONTO_VENTA NUMBER(10),
PRIMARY KEY (COD_VENTA),
FOREIGN KEY (COD_CLIENTE) REFERENCES CLIENTE,
FOREIGN KEY (COD_EMPLEADO) REFERENCES EMPLEADO,
FOREIGN KEY (COD_FACTURA) REFERENCES FACTURA,
FOREIGN KEY (COD_TEMPORADAS) REFERENCES TEMPORADA);
COMMIT;

CREATE TABLE VEHICULO
(
COD_VEHICULO NUMBER(7) NOT NULL,
COD_ESTADO NUMBER(7) NOT NULL,
COD_ADQUISICION NUMBER(7) NOT NULL,
COD_OFERTA NUMBER(7) NOT NULL,
COD_VENTA NUMBER(7) NOT NULL,
VEH_MARCA VARCHAR(50),
VEH_MODELO VARCHAR(50),
VEH_AÃ‘O DATE,
VEH_COLOR VARCHAR(50),
VEH_TIPO VARCHAR(50), 
VEH_KILOMETRAJE NUMBER(10),
VEH_PRECIO NUMBER(10),
PRIMARY KEY (COD_VEHICULO),
FOREIGN KEY (COD_ESTADO) REFERENCES ESTADO_VEHICULO,
FOREIGN KEY (COD_ADQUISICION) REFERENCES ADQUISICION,
FOREIGN KEY (COD_OFERTA) REFERENCES OFERTA,
FOREIGN KEY (COD_VENTA) REFERENCES VENTA);
COMMIT;

CREATE TABLE INVENTARIO_VEHICULO
(
COD_INV_VE NUMBER(7) NOT NULL,
COD_VEHICULO NUMBER(7) NOT NULL,
INV_UNIDADES_DISP NUMBER(10),
PRIMARY KEY (COD_INV_VE),
FOREIGN KEY (COD_VEHICULO) REFERENCES VEHICULO);
COMMIT;

CREATE TABLE TIPO_VENTA
(
COD_TIPO NUMBER(7) NOT NULL,
    COD_FINANCIAMIENTO NUMBER(7),
COD_VENTA NUMBER(7) NOT NULL,
TIP_TIPO VARCHAR(50),
TIP_MONTO_AL_CONTADO NUMBER(10),
TIP_DESCUENTO_CONTADO NUMBER(10),
PRIMARY KEY (COD_TIPO),
FOREIGN KEY (COD_VENTA) REFERENCES VENTA);
COMMIT; 



